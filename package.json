{
  "name": "chartist-projects",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "bootstrap": "^4.5.0",
    "chartist": "^0.10.1",
    "chartist-plugin-axistitle": "0.0.7",
    "chartist-plugin-tooltips-updated": "^0.0.20",
    "env-cmd": "^10.1.0",
    "react": "^16.13.1",
    "react-chartist": "^0.14.3",
    "react-dom": "^16.13.1",
    "react-scripts": "5.0.1",
    "reactstrap": "^8.5.1",
    "sass": "^1.72.0"
  },
  "scripts": {
    "start": "npm-run-all -p  watch-css start-js",
    "build-css": "sass --no-source-map --style=compressed src:src src/styles/styles.scss:src/styles/styles.css",
    "watch-css": "sass --no-source-map -w  src:src src/styles/styles.scss:src/styles/styles.css",
    "start-js": "react-scripts start",
    "build": "react-scripts build",
    "build:fed": "env-cmd -f .fed.env  react-scripts build",
    "build:inc": "env-cmd -f .inc.env  react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "jquery": "^3.5.1",
    "npm-run-all": "^4.1.5"
  }
}
